    <script>
        const view3D = new View3D("#el", {
            src: "https://modelos3d.s3.amazonaws.com/modelo<%= model %>.glb",
            webAR: {
                overlayRoot: ".view3d-ar-overlay",
                vertical: true
            },
            sceneViewer: {
                vertical: true
            },
            plugins: [new View3D.LoadingBar()]
        });
        view3D.loadPlugins(new View3D.ARButton());
        const buttons = [
            {
                el: document.getElementById("btn-alarm"),
                src: "https://modelos3d.s3.amazonaws.com/modelo1.glb",
            },
            {
                el: document.getElementById("btn-payphone"),
                src: "https://modelos3d.s3.amazonaws.com/modelo2.glb",
            },
            {
                el: document.getElementById("btn-plant"),
                src: "https://modelos3d.s3.amazonaws.com/modelo3.glb",
            },
        ];

        buttons.forEach((btn) => {
            btn.el.addEventListener("click", () => {
                view3D.load(btn.src);
            });
        });

        document.addEventListener('DOMContentLoaded', function () {
            // document.getElementById('loader').style.display = 'block';
            view3D.on("load", evt => {
                document.getElementById('loader').style.display = 'none';
            });
        });
    </script>
