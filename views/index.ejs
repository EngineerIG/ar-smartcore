    <style>
        .button {
            border-color: #dbdbdb;
            color: white;
            border-width: 1px;
            justify-content: center;
            padding: calc(0.5em - 1px) 1em;
            text-align: center;
            white-space: nowrap;
            box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.15);
            cursor: pointer;
            background-color: rgb(13, 71, 217);
        }

        .button:hover {
            box-shadow: 0px 10px 20px 2px rgba(0, 0, 0, 0.05);
        }

        .mr-2 {
            margin-right: 7px;
            margin-top: 6px;
        }

        .box {
            box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.3);
        }

        #fondo {
            background: white;
            padding: 0 10px;
        }

        #contenedor {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 10px 10px;
            background-color: rgb(13, 71, 217);
        }

        #loader {
            display: none;
            position: fixed;
            z-index: 9999;
            margin: auto;
            top: 0;
            left: 0;
            bottom: 0;
            right: 0;
            width: 60px;
            height: 60px;
            border: 6px solid #f3f3f3;
            border-radius: 50%;
            border-top: 6px solid rgb(13, 71, 217);
            width: 80px;
            height: 80px;
            -webkit-animation: spin 1s linear infinite;
            /* Safari */
            animation: spin 1s linear infinite;
            background-color: rgba(255, 255, 255, 0.7);
        }

        @-webkit-keyframes spin {
            0% {
                -webkit-transform: rotate(0deg);
            }

            100% {
                -webkit-transform: rotate(360deg);
            }
        }

        @keyframes spin {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
            }
        }
    </style>
